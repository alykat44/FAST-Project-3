{
    "name": "mern",
    "version": "1.0.0",
    "description": "Mern Demo",
    "main": "server.js",
    "scripts": {
        "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev && node scripts/seedDB.js",
        "start:prod": "node server.js",
        "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\" \" yarn seed\"",
        "client": "cd client && npm run start",
        "seed": "node scripts/seedDB.js",
        "install": "cd client && yarn install",
        "build": "cd client && npm run build",
        "heroku-postbuild": "npm run build"
    },
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "concurrently": "^3.5.0",
        "nodemon": "^1.19.4"
    },
    "dependencies": {
        "@material-ui/core": "^3.9.4",
        "@material-ui/icons": "^3.0.1",
        "axios": "^0.21.1",
        "bcryptjs": "^2.4.3",
        "body-parser": "^1.18.2",
        "connect-mongo": "^3.2.0",
        "express": "^4.17.3",
        "express-session": "^1.17.1",
        "if-env": "^1.0.4",
        "leaflet": "^1.7.1",
        "mapbox-gl": "^0.50.0-beta.1",
        "mongodb": "^3.6.4",
        "mongoose": "^5.11.15",
        "morgan": "^1.10.0",
        "nexmo": "^2.9.1",
        "nodemailer": "^4.6.8",
        "passport": "^0.4.1",
        "passport-local": "^1.0.0",
        "react": "^16.14.0",
        "react-dom": "^16.14.0",
        "react-geolocated": "^2.5.1",
        "react-map-gl": "^3.3.5",
        "react-mapbox-gl": "^3.9.2",
        "react-router-dom": "^4.3.1",
        "react-scripts": "^4.0.2",
        "router": "^1.3.5",
        "twilio": "^3.55.1",
        "whatwg-fetch": "^3.5.0"
    }
}
